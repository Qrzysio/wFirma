{
    "name": "webit/w-firma-api",
    "description": "wFirma.pl API",
    "type": "library",
    "license": "MIT",
    "keywords": ["sdk", "api", "wfirma", "w-firma"],
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3",
        "jms/serializer": "^1.0|^2.0|^3.0",
        "kriswallsmith/buzz": "^1.2",
        "nyholm/psr7": "^1.0",
        "psr/log": "^2.0|^3.0",
        "psr/simple-cache": "^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "monolog/monolog": "^3.0",
        "nette/php-generator": "^4.0",
        "fakerphp/faker": "^1.20.0",
        "phpspec/prophecy": "^1.20",
        "phpspec/prophecy-phpunit": "^2.3"
    },
    "suggest": {
        "symfony/cache": "To use VatCodeIdResolver with caching feature"
    },
    "autoload": {
        "psr-4": {
            "Webit\\WFirmaSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webit\\WFirmaSDK\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-1.x": "1.x-dev",
            "dev-master": "2.x-dev"
        }
    }
}
